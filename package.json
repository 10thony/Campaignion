{
  "name": "campaignion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:convex\"",
    "dev:frontend": "vite",
    "dev:convex": "convex dev",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts --run",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:e2e": "vitest --config vitest.integration.config.ts --run src/test/e2e/",
    "test:all": "npm run test && npm run test:integration",
    "integration-tests": "tsx scripts/run-integration-tests.ts"
  },
  "dependencies": {
    "@campaignion/shared-types": "file:./shared-types",
    "@clerk/clerk-react": "^5.37.0",
