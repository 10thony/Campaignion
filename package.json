    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "msw-trpc": "^2.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4",
    "ws": "^8.16.0"
  }
