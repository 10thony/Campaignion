    "pdfjs-dist": "^5.4.54",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "sonner": "^2.0.7",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^5.0.4",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
